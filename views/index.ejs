<% include ./partials/header %>

<h1>Hallo</h1>

<% blogs.forEach(function(blog){ %>
    <div>
        <h2><%= blog.title %></h2>
    </div>
    <img style="width: 400px; height: 200px" src="<%= blog.image %>">
    <span><%= blog.created %></span>
    <p><%= blog.body %></p>
    <a href="/blogs/<%= blog._id %>">Read More</a>
<% }) %>

<% include ./partials/footer %>